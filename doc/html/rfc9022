                      minOccurs=&quot;0&quot; /&gt;
             &lt;element name=&quot;disclose&quot;
                      type=&quot;contact:discloseType&quot;
                      minOccurs=&quot;0&quot; /&gt;
           &lt;/sequence&gt;
         &lt;/extension&gt;
       &lt;/complexContent&gt;
