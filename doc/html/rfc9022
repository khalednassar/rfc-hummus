            &lt;/rdeCsv:files&gt;
          &lt;/rdeCsv:csv&gt;
        &lt;/csvRegistrar:contents&gt;
        &lt;csvIDN:contents&gt;
          &lt;rdeCsv:csv name=&quot;idnLanguage&quot; sep=&quot;,&quot;&gt;
            &lt;rdeCsv:fields&gt;
              &lt;rdeCsv:fIdnTableId isRequired=&quot;true&quot;/&gt;
              &lt;rdeCsv:fUrl isRequired=&quot;true&quot;/&gt;
            &lt;/rdeCsv:fields&gt;
            &lt;rdeCsv:files&gt;
              &lt;rdeCsv:file
                cksum=&quot;D462EAD0&quot;&gt;
                idnLanguage-YYYYMMDD.csv
              &lt;/rdeCsv:file&gt;
            &lt;/rdeCsv:files&gt;
          &lt;/rdeCsv:csv&gt;
        &lt;/csvIDN:contents&gt;
        &lt;csvNNDN:contents&gt;
          &lt;rdeCsv:csv name=&quot;NNDN&quot; sep=&quot;,&quot;&gt;
            &lt;rdeCsv:fields&gt;
              &lt;csvNNDN:fAName/&gt;
              &lt;rdeCsv:fIdnTableId/&gt;
              &lt;csvNNDN:fOriginalName/&gt;
              &lt;csvNNDN:fNameState/&gt;
              &lt;csvNNDN:fMirroringNS/&gt;
              &lt;rdeCsv:fCrDate/&gt;
            &lt;/rdeCsv:fields&gt;
            &lt;rdeCsv:files&gt;
              &lt;rdeCsv:file
                cksum=&quot;11C80D60&quot;&gt;
                NNDN-YYYYMMDD.csv
              &lt;/rdeCsv:file&gt;
            &lt;/rdeCsv:files&gt;
          &lt;/rdeCsv:csv&gt;
        &lt;/csvNNDN:contents&gt;
        &lt;rdeEppParams:eppParams&gt;
           &lt;rdeEppParams:version&gt;1.0&lt;/rdeEppParams:version&gt;
           &lt;rdeEppParams:lang&gt;en&lt;/rdeEppParams:lang&gt;
           &lt;rdeEppParams:objURI&gt;urn:ietf:params:xml:ns:domain-1.0
           &lt;/rdeEppParams:objURI&gt;
           &lt;rdeEppParams:objURI&gt;urn:ietf:params:xml:ns:host-1.0
           &lt;/rdeEppParams:objURI&gt;
           &lt;rdeEppParams:objURI&gt;urn:ietf:params:xml:ns:contact-1.0
           &lt;/rdeEppParams:objURI&gt;
           &lt;rdeEppParams:svcExtension&gt;
             &lt;epp:extURI&gt;urn:ietf:params:xml:ns:secDNS-1.1
             &lt;/epp:extURI&gt;
             &lt;epp:extURI&gt;urn:ietf:params:xml:ns:rgp-1.0
             &lt;/epp:extURI&gt;
           &lt;/rdeEppParams:svcExtension&gt;
           &lt;rdeEppParams:dcp&gt;
             &lt;epp:access&gt;
               &lt;epp:all/&gt;
             &lt;/epp:access&gt;
             &lt;epp:statement&gt;
               &lt;epp:purpose&gt;
                 &lt;epp:admin/&gt;
                 &lt;epp:other/&gt;
                 &lt;epp:prov/&gt;
               &lt;/epp:purpose&gt;
               &lt;epp:recipient&gt;
                 &lt;epp:ours/&gt;
                 &lt;epp:public/&gt;
                 &lt;epp:unrelated/&gt;
               &lt;/epp:recipient&gt;
               &lt;epp:retention&gt;
                 &lt;epp:indefinite/&gt;
               &lt;/epp:retention&gt;
             &lt;/epp:statement&gt;
           &lt;/rdeEppParams:dcp&gt;
        &lt;/rdeEppParams:eppParams&gt;
      &lt;/rde:contents&gt;
   &lt;/rde:deposit&gt;

<span class="h2"><a class="selflink" id="section-17" href="#section-17">17</a>.  Example of a Differential Deposit Using the CSV Model</span>

   The following is an example of a Differential Deposit using the CSV
   model:

   &lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot;?&gt;
   &lt;rde:deposit
      xmlns:epp=&quot;urn:ietf:params:xml:ns:epp-1.0&quot;
      xmlns:rde=&quot;urn:ietf:params:xml:ns:rde-1.0&quot;
