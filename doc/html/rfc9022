                cksum=&quot;11C80D60&quot;&gt;
                NNDN-YYYYMMDD.csv
              &lt;/rdeCsv:file&gt;
            &lt;/rdeCsv:files&gt;
          &lt;/rdeCsv:csv&gt;
        &lt;/csvNNDN:contents&gt;
        &lt;rdeEppParams:eppParams&gt;
           &lt;rdeEppParams:version&gt;1.0&lt;/rdeEppParams:version&gt;
           &lt;rdeEppParams:lang&gt;en&lt;/rdeEppParams:lang&gt;
           &lt;rdeEppParams:objURI&gt;urn:ietf:params:xml:ns:domain-1.0
           &lt;/rdeEppParams:objURI&gt;
           &lt;rdeEppParams:objURI&gt;urn:ietf:params:xml:ns:host-1.0
           &lt;/rdeEppParams:objURI&gt;
           &lt;rdeEppParams:objURI&gt;urn:ietf:params:xml:ns:contact-1.0
           &lt;/rdeEppParams:objURI&gt;
           &lt;rdeEppParams:svcExtension&gt;
             &lt;epp:extURI&gt;urn:ietf:params:xml:ns:secDNS-1.1
             &lt;/epp:extURI&gt;
             &lt;epp:extURI&gt;urn:ietf:params:xml:ns:rgp-1.0
             &lt;/epp:extURI&gt;
           &lt;/rdeEppParams:svcExtension&gt;
           &lt;rdeEppParams:dcp&gt;
             &lt;epp:access&gt;
               &lt;epp:all/&gt;
             &lt;/epp:access&gt;
             &lt;epp:statement&gt;
               &lt;epp:purpose&gt;
                 &lt;epp:admin/&gt;
                 &lt;epp:other/&gt;
                 &lt;epp:prov/&gt;
               &lt;/epp:purpose&gt;
               &lt;epp:recipient&gt;
                 &lt;epp:ours/&gt;
                 &lt;epp:public/&gt;
                 &lt;epp:unrelated/&gt;
               &lt;/epp:recipient&gt;
               &lt;epp:retention&gt;
                 &lt;epp:indefinite/&gt;
               &lt;/epp:retention&gt;
             &lt;/epp:statement&gt;
           &lt;/rdeEppParams:dcp&gt;
        &lt;/rdeEppParams:eppParams&gt;
      &lt;/rde:contents&gt;
   &lt;/rde:deposit&gt;

