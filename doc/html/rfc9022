                      type=&quot;eppcom:labelType&quot; /&gt;
           &lt;attribute name=&quot;registrarId&quot;
                      type=&quot;positiveInteger&quot; /&gt;
         &lt;/extension&gt;
       &lt;/simpleContent&gt;
     &lt;/complexType&gt;
   &lt;/schema&gt;
