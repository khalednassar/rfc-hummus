         &lt;minLength value=&quot;1&quot; /&gt;
         &lt;maxLength value=&quot;255&quot; /&gt;
       &lt;/restriction&gt;
     &lt;/simpleType&gt;
     &lt;simpleType name=&quot;optPostalLineType&quot;&gt;
       &lt;restriction base=&quot;normalizedString&quot;&gt;
         &lt;maxLength value=&quot;255&quot; /&gt;
       &lt;/restriction&gt;
     &lt;/simpleType&gt;
     &lt;simpleType name=&quot;pcType&quot;&gt;
       &lt;restriction base=&quot;token&quot;&gt;
         &lt;maxLength value=&quot;16&quot; /&gt;
       &lt;/restriction&gt;
     &lt;/simpleType&gt;
     &lt;simpleType name=&quot;ccType&quot;&gt;
       &lt;restriction base=&quot;token&quot;&gt;
         &lt;length value=&quot;2&quot; /&gt;
       &lt;/restriction&gt;
     &lt;/simpleType&gt;
     &lt;complexType name=&quot;whoisInfoType&quot;&gt;
       &lt;sequence&gt;
         &lt;element name=&quot;name&quot;
                  type=&quot;eppcom:labelType&quot;
                  minOccurs=&quot;0&quot; /&gt;
         &lt;element name=&quot;url&quot;
                  type=&quot;anyURI&quot;
                  minOccurs=&quot;0&quot; /&gt;
       &lt;/sequence&gt;
     &lt;/complexType&gt;
     &lt;!-- Delete Type --&gt;
     &lt;complexType name=&quot;deleteType&quot;&gt;
       &lt;complexContent&gt;
         &lt;extension base=&quot;rde:deleteType&quot;&gt;
           &lt;sequence&gt;
             &lt;element name=&quot;id&quot;
                      type=&quot;eppcom:clIDType&quot;
                      minOccurs=&quot;0&quot;
                      maxOccurs=&quot;unbounded&quot; /&gt;
           &lt;/sequence&gt;
         &lt;/extension&gt;
       &lt;/complexContent&gt;
     &lt;/complexType&gt;
   &lt;/schema&gt;
